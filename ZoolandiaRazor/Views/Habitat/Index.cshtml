
@{
    ViewBag.Title = "Index";
}

<h2>Habitat</h2>

<div class="row">
    <div class="col-md-6 col-md-offset-3">
        <ul>
            @{ 
                foreach (var habitat in ViewBag.All_Habitats)
                {
                    <li><p>habitat.Name</p></li>
                }
            }
        </ul>
    </div>
</div>